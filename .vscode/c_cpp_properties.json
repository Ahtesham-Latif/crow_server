{
    "version": 4,
    "configurations": [
        {
            "name": "MinGW64",
            "compilerPath": "D:/msys64/mingw64/bin/g++.exe",
            "intelliSenseMode": "windows-gcc-x64",
            "cStandard": "c17",
            "cppStandard": "c++17",
            "browse": {
                "path": [
                    "${workspaceFolder}",
                    "${workspaceFolder}/Crow/include",
                    "${workspaceFolder}/httplib",
                    "${workspaceFolder}/controllers",
                    "${workspaceFolder}/models",
                    "${workspaceFolder}/public",
                    "${workspaceFolder}/db"
