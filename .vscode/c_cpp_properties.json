                "limitSymbolsToIncludedHeaders": false,
                "databaseFilename": ""
            },
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/Crow/include",
                "${workspaceFolder}/httplib",
                "${workspaceFolder}/controllers",
                "${workspaceFolder}/models"
            ],
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE",
                "_WIN32_WINNT=0x0A00"
            ]
        }
    ]
}
